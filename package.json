{
  "name": "@brandcast_app/mealviewer-api-client",
  "version": "0.1.0",
  "description": "TypeScript/JavaScript client for MealViewer School Lunch Menu API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "mealviewer",
    "school-lunch",
    "menu",
    "api-client",
    "typescript",
    "javascript",
    "familycast",
    "brandcast"
  ],
  "author": "BrandCast Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrandCast-Signage/mealviewer-api-client.git"
  },
  "bugs": {
    "url": "https://github.com/BrandCast-Signage/mealviewer-api-client/issues"
  },
  "homepage": "https://github.com/BrandCast-Signage/mealviewer-api-client#readme",
  "dependencies": {
    "axios": "^1.7.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/src/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
